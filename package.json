{
  "name": "sass-7-1-project",
  "version": "1.0.0",
  "description": "Proyecto de interfaz web utilizando SASS con el patr√≥n 7-1",
  "main": "index.html",
  "scripts": {
    "sass": "sass sass/style.scss css/style.css",
    "sass:watch": "sass --watch sass/style.scss css/style.css",
    "sass:build": "sass sass/style.scss css/style.css --style compressed",
    "dev": "sass --watch sass/style.scss css/style.css --source-map",
    "build": "sass sass/style.scss css/style.css --style compressed --no-source-map",
    "serve": "live-server --port=3000 --host=localhost"
  },
  "keywords": [
    "sass",
    "css",
    "7-1",
    "pattern",
    "web",
    "frontend"
  ],
  "author": "Tu Nombre",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.0",
    "sass": "^1.69.5"
  }
}
